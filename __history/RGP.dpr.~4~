program RGP;

uses
  System.StartUpCopy,
  FMX.Forms,
  U_Log in 'U_Log.pas' {Log},
  U_Main in 'U_Main.pas' {Main},
  U_Ins in 'U_Ins.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.CreateForm(TLog, Log);
  Application.CreateForm(TMain, Main);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
